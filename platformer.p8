local start_screen_time=0
local start_screen_duration=7

local loading_text="loading game"
local loading_dots=""
local loading_dot_interval=0.5
local loading_dot_timer=0

function show_start_screen()
		game.update=start_screen_update
		game.draw=start_screen_draw
		start_screen_time=0
end

function start_screen_update()

		if btn(➡️) then
		--move to next stage
				show_menu()
		end
		
		start_screen_time=start_screen_time+1/30
		
		if start_screen_time>=start_screen_duration then
				show_menu()
		end
end

function start_screen_draw()
		cls()
		
		camera(0,0)
  map(16,16)
  
		print(loading_text .. loading_dots,40,110,2)
		loading_dot_timer+=1/30
		
		if loading_dot_timer>=loading_dot_interval then
				loading_dot_timer=0
				loading_dots=loading_dots .. "."
		end
		
		if #loading_dots>3 then
				loading_dots=""
		end	
end
