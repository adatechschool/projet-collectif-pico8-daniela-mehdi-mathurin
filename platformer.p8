function upickups()
		if pu.act then
 			if abs(player.x-pu.x)<=4 and abs(player.y-pu.y)<=4 then
    		pu.act=false
    		player.haskey=true
    		sfx(06)
 			end 
 	end
end

--affichage   
function dpickups()
